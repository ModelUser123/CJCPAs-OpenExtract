{
  "template_id": "1099-misc",
  "template_name": "Form 1099-MISC - Miscellaneous Income",
  "description": "IRS Form 1099-MISC for reporting miscellaneous income including rents, royalties, prizes, and other payments.",
  "document_type": "tax-form",
  "version": "1.0.0",
  "author": "OpenExtract Community",
  "tags": ["1099", "tax", "irs", "income", "miscellaneous"],
  "fields": [
    {
      "field_name": "payer_name",
      "display_name": "Payer's Name",
      "data_type": "string",
      "required": true,
      "extraction_method": "regex",
      "regex_pattern": "(?:PAYER'?S?\\s+name|Payer)[:\\s]+([A-Za-z0-9\\s\\-\\.,&'()]+?)(?:\\n|$|\\s{2,})",
      "section": "Payer Information"
    },
    {
      "field_name": "payer_tin",
      "display_name": "Payer's TIN",
      "data_type": "string",
      "required": true,
      "extraction_method": "regex",
      "regex_pattern": "(?:PAYER'?S?\\s+TIN|Payer'?s?\\s+(?:federal\\s+)?(?:identification|ID)\\s+(?:number|no\\.?))[:\\s]*(\\d{2}[- ]?\\d{7})",
      "validation": "^\\d{2}-?\\d{7}$",
      "section": "Payer Information"
    },
    {
      "field_name": "payer_address",
      "display_name": "Payer's Address",
      "data_type": "string",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:PAYER'?S?\\s+(?:street\\s+)?address)[:\\s]+([A-Za-z0-9\\s\\-\\.,#]+)",
      "section": "Payer Information"
    },
    {
      "field_name": "recipient_name",
      "display_name": "Recipient's Name",
      "data_type": "string",
      "required": true,
      "extraction_method": "regex",
      "regex_pattern": "(?:RECIPIENT'?S?\\s+name|Recipient)[:\\s]+([A-Za-z0-9\\s\\-\\.,&'()]+?)(?:\\n|$|\\s{2,})",
      "section": "Recipient Information"
    },
    {
      "field_name": "recipient_tin",
      "display_name": "Recipient's TIN",
      "data_type": "string",
      "required": true,
      "extraction_method": "regex",
      "regex_pattern": "(?:RECIPIENT'?S?\\s+TIN|Recipient'?s?\\s+(?:identification|ID)\\s+(?:number|no\\.?))[:\\s]*(\\d{2}[- ]?\\d{7}|\\d{3}[- ]?\\d{2}[- ]?\\d{4})",
      "section": "Recipient Information"
    },
    {
      "field_name": "recipient_address",
      "display_name": "Recipient's Address",
      "data_type": "string",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:RECIPIENT'?S?\\s+(?:street\\s+)?address)[:\\s]+([A-Za-z0-9\\s\\-\\.,#]+)",
      "section": "Recipient Information"
    },
    {
      "field_name": "box_1_rents",
      "display_name": "Box 1 - Rents",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:1\\s+)?[Rr]ents[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Income"
    },
    {
      "field_name": "box_2_royalties",
      "display_name": "Box 2 - Royalties",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:2\\s+)?[Rr]oyalties[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Income"
    },
    {
      "field_name": "box_3_other_income",
      "display_name": "Box 3 - Other Income",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:3\\s+)?[Oo]ther\\s+income[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Income"
    },
    {
      "field_name": "box_4_federal_tax_withheld",
      "display_name": "Box 4 - Federal Tax Withheld",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:4\\s+)?[Ff]ederal\\s+(?:income\\s+)?tax\\s+withheld[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Withholding"
    },
    {
      "field_name": "box_5_fishing_boat_proceeds",
      "display_name": "Box 5 - Fishing Boat Proceeds",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:5\\s+)?[Ff]ishing\\s+boat\\s+proceeds[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Income"
    },
    {
      "field_name": "box_6_medical_payments",
      "display_name": "Box 6 - Medical/Health Payments",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:6\\s+)?[Mm]edical(?:\\s+and\\s+health\\s+care)?\\s+payments[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Income"
    },
    {
      "field_name": "box_8_substitute_payments",
      "display_name": "Box 8 - Substitute Payments",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:8\\s+)?[Ss]ubstitute\\s+payments[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Income"
    },
    {
      "field_name": "box_10_crop_insurance",
      "display_name": "Box 10 - Crop Insurance Proceeds",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:10\\s+)?[Cc]rop\\s+insurance\\s+proceeds[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Income"
    },
    {
      "field_name": "box_11_foreign_tax_paid",
      "display_name": "Box 11 - Foreign Tax Paid",
      "data_type": "currency",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:11\\s+)?[Ff]oreign\\s+tax\\s+paid[:\\s]*\\$?([\\d,]+(?:\\.\\d{2})?)",
      "section": "Withholding"
    },
    {
      "field_name": "tax_year",
      "display_name": "Tax Year",
      "data_type": "string",
      "required": false,
      "extraction_method": "regex",
      "regex_pattern": "(?:Tax\\s+[Yy]ear|Form\\s+1099-MISC\\s+)(20\\d{2})",
      "fallback_patterns": [
        "(20\\d{2})\\s+Form\\s+1099"
      ],
      "section": "Form Information"
    }
  ],
  "output_format": {
    "csv_headers": [
      "payer_name",
      "payer_tin",
      "payer_address",
      "recipient_name",
      "recipient_tin",
      "recipient_address",
      "box_1_rents",
      "box_2_royalties",
      "box_3_other_income",
      "box_4_federal_tax_withheld",
      "box_5_fishing_boat_proceeds",
      "box_6_medical_payments",
      "box_8_substitute_payments",
      "box_10_crop_insurance",
      "box_11_foreign_tax_paid",
      "tax_year"
    ],
    "date_format": "YYYY-MM-DD"
  }
}
